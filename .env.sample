[app]
debug = false
trace = false

[database]
hostname = 127.0.0.1
database = fastadmin
username = root
password = root
hostport = 3306
prefix = fa_

[wechat_miniprogram]
; 微信小程序配置
; 从微信公众平台获取: https://mp.weixin.qq.com/
; 路径: 开发 -> 开发管理 -> 开发设置
app_id = 
app_secret = 

[wechat_payment]
; 微信支付配置（核销券订单支付）
; 所有值都需要在微信商户平台获取
appid = 
mch_id = 
api_key = 
apiv3_key = 
cert_path = 
key_path = 
serial_no = 
; 回调地址必须为 HTTPS 公网地址
platform_cert_path = 
notify_url = https://yourdomain.com/api/wanlshop/voucher/order/notify
refund_notify_url = https://yourdomain.com/api/wanlshop/voucher/order/refundNotify
transfer_notify_url = https://yourdomain.com/api/wanlshop/voucher/settlement/transferNotify

[voucher]
; 核销券有效期（天）
valid_days = 30
; 是否允许退款（true/false）
allow_refund = true
; 仅在到期前多少天内允许申请退款
refund_days = 7
